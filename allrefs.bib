journal = {J. Chem. Phys.},
month = {sep},
number = {9},
pages = {094104},
pmid = {17824729},
title = {{On the vibronic coupling approximation: A generally applicable approach for determining fully quadratic quasidiabatic coupled electronic state Hamiltonians}},
url = {http://aip.scitation.org/doi/10.1063/1.2756540},
volume = {127},
year = {2007}
}

