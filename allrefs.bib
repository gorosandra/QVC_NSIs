@article{Serrano-Andres2006,
abstract = {Distinct photophysical behavior of nucleobase adenine and its constitutional isomer, 2-aminopurine, has been studied by using quantum chemical methods, in particular an accurate ab initio multiconfigurational second-order perturbation theory. After light irradiation, the efficient, ultrafast energy dissipation observed for nonfluorescent 9H-adenine is explained here by the nonradiative internal conversion process taking place along a barrierless reaction path from the initially populated 1(pipi* La) excited state toward a low-lying conical intersection (CI) connected with the ground state. In contrast, the strong fluorescence recorded for 2-aminopurine at 4.0 eV with large decay lifetime is interpreted by the presence of a minimum in the 1(pipi* La) hypersurface lying below the lowest CI and the subsequent potential energy barrier required to reach the funnel to the ground state. Secondary deactivation channels were found in the two systems related to additional CIs involving the 1(pipi* Lb) and 1(npi*) states. Although in 9H-adenine a population switch between both states is proposed, in 7H-adenine this may be perturbed by a relatively larger barrier to access the 1(npi*) state, and, therefore, the 1(pipi* Lb) state becomes responsible for the weak fluorescence measured in aqueous adenine at approximately 4.5 eV. In contrast to previous models that explained fluorescence quenching in adenine, unlike in 2-aminopurine, on the basis of the vibronic coupling of the nearby 1(pipi*) and 1(npi*) states, the present results indicate that the 1(npi*) state does not contribute to the leading photophysical event and establish the prevalence of a model based on the CI concept in modern photochemistry.},
author = {Serrano-Andres, L. and Merchan, M. and Borin, A. C.},
doi = {10.1073/pnas.0602991103},
file = {:home/felix/ownCloud2/Literatur/Diss/DNA Exc/Serrano-Andres - 2006 - Adenine.pdf:pdf},
isbn = {0027-8424 (Print){\$}\backslash{\$}n0027-8424 (Linking)},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences},
number = {23},
pages = {8691--8696},
pmid = {16731617},
title = {{Adenine and 2-aminopurine: Paradigms of modern theoretical photochemistry}},
url = {http://www.pnas.org/cgi/doi/10.1073/pnas.0602991103},
volume = {103},
year = {2006}
}

@article{Barbatti2012,
abstract = {In spite of the importance of nonadiabatic dynamics simulations for the understanding of ultrafast photo-induced phenomena, simulations based on different methodologies have often led to contradictory results. In this work, we proceed through a comprehensive investigation of on-the-fly surface-hopping simulations of 9H-adenine in the gas phase using different electronic structure theories (ab initio, semi-empirical, and density functional methods). Simulations that employ ab initio and semi-empirical multireference configuration interaction methods predict the experimentally observed ultrafast deactivation of 9H-adenine with similar time scales, however, through different internal conversion channels. Simulations based on time-dependent density functional theory with six different hybrid and range-corrected functionals fail to predict the ultrafast deactivation. The origin of these differences is analyzed by systematic calculations of the relevant reaction pathways, which show that these discrepancies can always be traced back to topographical features of the underlying potential energy surfaces.},
author = {Barbatti, Mario and Lan, Zhenggang and Crespo-Otero, Rachel and Szymczak, Jaroslaw J. and Lischka, Hans and Thiel, Walter},
doi = {10.1063/1.4731649},
file = {:home/felix/ownCloud2/Literatur/Diss/DNA Exc/Barbatti - 2012 - Adenine comp.pdf:pdf},
isbn = {0021-9606},
issn = {00219606},
journal = {Journal of Chemical Physics},
number = {22},
pmid = {23249040},
title = {{Critical appraisal of excited state nonadiabatic dynamics simulations of 9H-adenine}},
volume = {137},
year = {2012},
pages = {22A503}
}

