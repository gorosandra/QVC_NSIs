@article{VanderHorst1999,
abstract = {We present an calculation of the electronic and optical excitations of an isolated polythiophene chain as well as of bulk polythiophene. We use the GW approximation for the electronic self-energy and include excitonic effects by solving the electron-hole Bethe-Salpeter equation. The inclusion of interchain screening in the case of bulk polythiophene drastically reduces both the quasi-particle band gap and the exciton binding energies, but the optical gap is hardly affected. This finding is relevant for conjugated polymers in general.},
